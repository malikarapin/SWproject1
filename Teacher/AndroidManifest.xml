<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="acsm.teacher"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="acsm.teacher.TeacherLogin"
            android:label="@string/app_name" 
            android:screenOrientation="nosensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="acsm.teacher.TeacherMenu"
            android:label="@string/title_activity_teacher_menu" 
            android:screenOrientation="nosensor">
        </activity>
        <activity
            android:name="acsm.teacher.TeacherCheck"
            android:label="@string/title_activity_teacher__check"
            android:screenOrientation="nosensor">
        </activity>
        <activity
            android:name="acsm.teacher.TeaxherQuestion"
            android:label="@string/title_activity_teaxher_question" 
            android:screenOrientation="nosensor">
        </activity>
        <activity
            android:name="acsm.teacher.TeacherViewReport"
            android:label="@string/title_activity_teacher_view_report" 
            android:screenOrientation="nosensor">
        </activity>
    </application>

</manifest>
