<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="acsm.teacher"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Teacherspl"
            android:label="@string/title_activity_teacherspl"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TeacherMenu"
            android:label="@string/title_activity_teacher_menu" >
        </activity>
        <activity
            android:name=".TeacherCheck"
            android:label="@string/title_activity_teacher__check" >
        </activity>
        <activity
            android:name=".TeaxherQuestion"
            android:label="@string/title_activity_teaxher_question" >
        </activity>
        <activity
            android:name=".TeacherViewReport"
            android:label="@string/title_activity_teacher_view_report" >
        </activity>
        <activity
            android:name=".TeacherLogin"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".TeacherChangePassword"
            android:label="@string/title_activity_teacher_change_password" >
        </activity>
    </application>

</manifest>